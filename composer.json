{
  "name": "k1low/property-enum",
  "description": "Property based enum plugin for CakePHP 3",
  "type": "cakephp-plugin",
  "keywords": [
    "cakephp",
    "enum"
  ],
  "homepage": "https://github.com/k1low/property-enum",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "cakephp/cakephp": "^4.0",
    "cakedc/enum": ">=1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "cakephp/cakephp-codesniffer": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "PropertyEnum\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PropertyEnum\\Test\\": "tests"
    }
  }
}
